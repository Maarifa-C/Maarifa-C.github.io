{
    "manifest_version": 3,
    "name": "chatGPT",
    "version": "1.0.0",
     "permissions": ["activeTab", "identity", "identity.email", "tabs"], 

    "action": {
      "default_popup": "index.html"
    },
  
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["scripts.js"]
  } ]
  }